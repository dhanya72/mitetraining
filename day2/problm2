#include <stdio.h>

int main() {
    int n, weight, accepted_count = 0, rejected_count = 0;

    printf("Enter number of products: ");
    scanf("%d", &n);

for (int i = 1; i < n; i++) {
    printf("Enter weights (in grams): ");
        scanf("%d", &weight);
        if (weight >= 950 && weight <= 1050) {
            accepted_count++;
        } else {
            rejected_count++;
        }
    }

    printf("Accepted Products: %d\n", accepted_count);
    printf("Rejected Products: %d\n", rejected_count);

    return 0;
}


